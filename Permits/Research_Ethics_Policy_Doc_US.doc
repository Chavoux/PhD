\pard\plain\ltrpar\qj\fi600\sa120\s27\sl360\slmult1\itap0{\s27\f0\fs20\b\lang7177{\*\listtag0}\abinodiroverride\ltrch B2. RESEARCH INVOLVING THE CARE AND USE OF ANIMALS}{\s27\f0\fs20\b\lang7177{\*\listtag0}\par}
\pard\plain\ltrpar\ql\fi-481\li1077\sa120\s27\sl360\slmult1\itap0\tx1077{\s27\f0\fs20\b\lang7177{\*\listtag0}\abinodiroverride\ltrch B3.\tab RESEARCH INVOLVING ENVIRONMENT- AND BIO-SAFETY}{\s27\f0\fs20\b\lang7177{\*\listtag0}\par}
